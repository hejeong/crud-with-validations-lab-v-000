<h1>New Song Form</h1>
<%= form_for(@song) do |f|%>
  Title: <%= f.text_field :title %> <br>
  <br>
  Released:
  <%= f.radio_button :released, 'true' %>
  <%= f.label "True" %>
  <%= f.radio_button :released, 'false' %>
  <%= f.label "False" %> <br>
  <br>
  Release Year: <%= f.text_field :release_year %> <br>
  <br>
  Artist Name: <%= f.text_field :artist_name %> <br>
  <br>
  Genre: <%= f.text_field :genre %> <br>
  <br>
  <%= f.submit "Submit" %>
<% end %>
